<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zsh.app.accountscollect">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_appicon"
        android:label="账号集"
        android:roundIcon="@mipmap/ic_account"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity"></activity>
        <activity android:name=".AddAccountActivity"></activity>
        <activity android:name=".ShowAccountActivity"></activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".RegisterActivity"></activity>
        <activity android:name=".ChangePswActivity"></activity>
    </application>

</manifest>